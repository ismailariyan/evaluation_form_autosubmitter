{
    "manifest_version": 3,
    "name": "uHate uCam",
    "version": "1.0",
    "description": "i know we all hate doing this, but what needs to be done has be done",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
       "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
      }
    }
  }
  